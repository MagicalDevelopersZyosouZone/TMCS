[
  //Account
  {
    "Register": {
      "params": [
        "userId",
        "publicKey",
        "[passwordHash]"
      ],
      "returns": [ "[errorCode]", "message" ]
    },

    "GetLoginInfo": {
      "params": [ "userId" ],
      "returns": [
        "errorCode",
        "message",
        "authType",
        "code"
      ]
    },

    "Login": {
      "params": [ "userId", "passwordHash" ],
      "returns": [ "errorCode", "message", "token" ]
    },

    "GetInfo": {
      "params": [ "userId" ],
      "returns": [
        "errorCode",
        "message",
        {
          "name": "userInfo",
          "members": [ "userId", "name", "faceUrl" ]
        }
      ]
    },

    //TODO
    "SetInfo": {
      "params": [
        "userId",
        {
          "name": "userInfo",
          "members": [ "name", "faceUrl" ]
        }
      ],
      "returns": [ "errorCode", "message" ]
    }
  },

  //TMCS
  {
    "Login": {
      "params": [ "userId", "token" ],
      "returns": [ "errorCode", "message" ]
    },

    "Logout": {
      "params": null,
      "returns": [ "errorCode", "message" ]
    },

    "GetFriends": {
      "params": null,
      "returns": [
        "errorCode",
        "message",
        {
          "name": "friends",
          "type": "array",
          "item": [ "userId" ]
        }
      ]
    },

    "AddFriend": {
      "params": [ "targetuserId" ],
      "returns": [ "errorCode", "message" ]
    },

    "SearchUsers": {
      "params": [ "targetName" ],
      "returns": [
        "errorCode",
        "message",
        {
          "name": "users",
          "type": "array",
          "item": [ "userId", "name" ]
        }
      ]
    },

    "GetMessages": {
      "params": [ "targetuserId", "timestamp", "count" ],
      "returns": [
        "errorCode",
        "message",
        {
          "name": "massages",
          "type": "array",
          "item": [ "timestamp", "messageText" ]
        }
      ]
    },

    "SendMessages": {
      "params": [
        "targetuserId",
        {
          "name": "messages",
          "type": "array",
          "item": [ "relativeTime", "messageText" ]
        }
      ],
      "returns": [ "errorCode", "message" ]
    },

    "PushMessages": {
      "params": {
        "name": "messages",
        "type": "array",
        "item": [ "senderuserId", "timestamp", "messageText" ]
      }
    },

    //TODO
    "DeleteFriend": {
      "params": [ "targetuserId" ],
      "returns": [ "errorCode", "message" ]
    }
  }
]