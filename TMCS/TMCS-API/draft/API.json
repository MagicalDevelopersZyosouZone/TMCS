[
  //Account
  {
    "Register": {
      "params": [
        "uid",
        "pwdEncrypted",
        {
          "name": "userInfo",
          "members": [ "uid", "name", "faceUrl" ]
        }
      ],
      "returns": [ "errCode", "msg" ]
    },

    "GetLoginInfo": {
      "params": [ "uid" ],
      "returns": [
        "errCode",
        "msg",
        "authType",
        "code"
      ]
    },

    "Login": {
      "params": [ "uid", "pwdEncrypted" ],
      "returns": [ "errCode", "msg", "token" ]
    },

    "GetInfo": {
      "params": [ "uid" ],
      "returns": [
        "errCode",
        "msg",
        {
          "name": "userInfo",
          "members": [ "uid", "name", "faceUrl" ]
        }
      ]
    },

    //TODO
    "SetInfo": {
      "params": [
        "uid",
        {
          "name": "userInfo",
          "members": [ "name", "faceUrl" ]
        }
      ],
      "returns": [ "errCode", "msg" ]
    }
  },

  //TMCS
  {
    "Login": {
      "params": [ "uid", "token" ],
      "returns": [ "errCode", "msg" ]
    },

    "Logout": {
      "params": null,
      "returns": [ "errCode", "msg" ]
    },

    "GetFriends": {
      "params": null,
      "returns": [
        "errCode",
        "msg",
        {
          "name": "friends",
          "type": "array",
          "item": [ "uid" ]
        }
      ]
    },

    "AddFriend": {
      "params": [ "targetUid" ],
      "returns": [ "errCode", "msg" ]
    },

    "SearchUsers": {
      "params": [ "targetName" ],
      "returns": [
        "errCode",
        "msg",
        {
          "name": "users",
          "type": "array",
          "item": [ "uid", "name" ]
        }
      ]
    },

    "GetMessages": {
      "params": [ "targetUid", "timestamp", "count" ],
      "returns": [
        "errCode",
        "msg",
        {
          "name": "massages",
          "type": "array",
          "item": [ "timestamp", "messageText" ]
        }
      ]
    },

    "SendMessages": {
      "params": [
        "targetUid",
        {
          "name": "messages",
          "type": "array",
          "item": [ "relativeTime", "messageText" ]
        }
      ],
      "returns": [ "errCode", "msg" ]
    },

    "PushMessages": {
      "params": {
        "name": "messages",
        "type": "array",
        "item": [ "senderUid", "timestamp", "messageText" ]
      }
    },

    //TODO
    "DeleteFriend": {
      "params": [ "targetUid" ],
      "returns": [ "errCode", "msg" ]
    }
  }
]